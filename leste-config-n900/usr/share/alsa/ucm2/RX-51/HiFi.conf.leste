SectionDevice."Speaker" {
	Comment "Internal Speaker"
	EnableSequence [
		cset "name='Speaker Function' On"
#		cset "name='Jack Function' Off"

	]

	DisableSequence [
		cset "name='Speaker Function' Off"
#		cset "name='Jack Function' Off"
	]

	Value {
		PlaybackVolume "PCM Playback Volume"
		PlaybackChannels 2
		PlaybackPriority 500
		PlaybackPCM "hw:${CardId},0"
#		CapturePCM "hw:${CardId},0"

	}
}

SectionDevice."FMTX" {
        Comment "Internal FM Transmitter"
        EnableSequence [
                cset "name='FM Transmitter Switch' on"
                cset "name='Jack Function' Headphone"
                cset "name='Speaker Function' Off"
#               cset "name='Jack Function' Off"

        ]

        DisableSequence [
                cset "name='FM Transmitter Switch' off"
#               cset "name='Speaker Function' Off"
                cset "name='Jack Function' Off"
        ]

        Value {
                PlaybackVolume "PCM Playback Volume"
                PlaybackChannels 2
                PlaybackPriority 200
                PlaybackPCM "hw:${CardId},0"
 #               CapturePCM "hw:${CardId},0"
	}
}


SectionDevice."Headphones" {
	Comment "Headset"
	EnableSequence [
		cset "name='Speaker Function' Off"
		cset "name='Earphone Switch' off"
		cset "name='Jack Function' Headphone"
		cset "name='TPA6130A2 Headphone Playback Volume' 25"
	]

	DisableSequence [
		cset "name='Speaker Function' On"
		cset "name='Jack Function' Off"
		cset "name='TPA6130A2 Headphone Playback Volume' 7"
	]

	Value {
		PlaybackVolume "PCM Playback Volume"
		PlaybackChannels 2
		PlaybackPriority 100
		PlaybackPCM "hw:${CardId},0"
#		CapturePCM "hw:${CardId},0"		
		JackControl "AV Jack"

	}
}

